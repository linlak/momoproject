{
    "name": "linlak/momoproject",
    "description": "This is a template project to enable you get started with momo api integration using linlak/momoapi",
    "keywords": [
         "momo api project","linlak","linus"
      ],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Linus Nowomukama",
            "email": "nowomukama.linlak.linus2@gmail.com"
        }
    ],
    "type": "project",
    "require": {
		"linlak/momoapi":"^1.0.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "prefer-stable": true,
	"autoload": {
        "psr-4": {
            "MomoProject\\": "src/"
        }
    },
  "config":{
    "preferred-install": "dist"
  },
    "suggest": {
        "linlak/momoapi": "Required for momo api integration"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
